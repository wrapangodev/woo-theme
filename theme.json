{
	"version": 2,
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": [
				"page",
				"post"
			]
		},
		{
			"name": "page",
			"title": "Page",
			"postTypes": [
				"page"
			]
		},
		{
			"name": "page-blank",
			"title": "Page (No Title)",
			"postTypes": [
				"page"
			]
		},
		{
			"name": "single",
			"title": "Single Post",
			"postTypes": [
				"post"
			]
		},
		{
			"name": "single-product",
			"title": "Single Product",
			"postTypes": [
				"product"
			]
		},
		{
			"name": "archive-product",
			"title": "Products Catalog",
			"postTypes": []
		},
		{
			"name": "taxonomy-product_cat",
			"title": "Products by Categories",
			"postTypes": []
		},
		{
			"name": "taxonomy-product_tag",
			"title": "Products by Tags",
			"postTypes": []
		},
		{
			"name": "product-search-results",
			"title": "Product Search Results",
			"postTypes": []
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"slug": "primary",
					"color": "#2b2b2b",
					"name": "Primary"
				},
				{
					"slug": "second",
					"color": "#c94917",
					"name": "Second"
				},
				{
					"slug": "grey",
					"color": "#e0e0e0",
					"name": "Grey"
				},
				{
					"slug": "lite-grey",
					"color": "#505050",
					"name": "Lite Grey"
				},
				{
					"slug": "lite",
					"color": "#F8F8F8",
					"name": "Lite"
				},
				{
					"slug": "white",
					"color": "#FFFFFF",
					"name": "White"
				}
			]
		},
		"custom": {
			"spacing": {
				"horizontal": "20px",
				"small": "max(1.25rem, 5vw)",
				"medium": "clamp(2rem, 8vw, calc(4 * var(--wp--style--block-gap)))",
				"large": "clamp(4rem, 10vw, 8rem)",
				"outer": "var(--wp--custom--spacing--small, 1.25rem)"
			},
			"typography": {
				"font-size": {
					"heading-1": "clamp(2.25rem, 5vw, 3.25rem)",
					"heading-2": "clamp(1.875rem, 4vw, 2.625rem)",
					"heading-3": "clamp(1.5rem, 3vw, 2rem)",
					"heading-4": "clamp(1.25rem, 3vw, 1.75rem)",
					"heading-5": "clamp(1.125rem, 3vw, 1.5rem)",
					"heading-6": "clamp(1rem, 2vw, 1.25rem)",
					"body": "0.875rem"
				},
				"line-height": {
					"small": 1.2,
					"medium": 1.3,
					"normal": 1.6,
					"large": 1.8
				},
				"letter-spacing": {
					"heading-big": "-0.04em",
					"heading-normal": "-0.02em",
					"button": "0.08em",
					"menu": "0em",
					"body": "0.02em"
				}
			}
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "\"firava\", sans-serif",
					"name": "firava",
					"slug": "primary-font",
					"fontFace": [
						{
							"fontFamily": "firava",
							"fontWeight": "100 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Fira_Sans/Firava.woff2" ]
						},
						{
							"fontFamily": "firava",
							"fontWeight": "100 900",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Fira_Sans/FiravaItalic.woff2" ]
						}
					]
				}
			],
			"fontSizes": [
				{
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"size": "1.125rem",
					"slug": "medium"
				},
				{
					"size": "1.25rem",
					"slug": "large"
				},
				{
					"size": "1.5rem",
					"slug": "x-large"
				}
			]
		},
		"layout": {
			"contentSize": "1170px",
			"wideSize": "1536px"
		},
		"blocks": {
			"core/heading": {
				"typography": {
					"fontSizes": [
						{
							"size": "2.125rem",
							"slug": "x-small"
						},
						{
							"size": "clamp(2.125rem, 10vw, 2.375rem)",
							"slug": "small"
						},
						{
							"size": "clamp(2.125rem, 10vw, 3rem)",
							"slug": "large"
						},
						{
							"size": "clamp(2.125rem, 10vw, 3.25rem)",
							"slug": "x-large"
						}
					]
				}
			},
			"core/post-title": {
				"typography": {
					"fontSizes": [
						{
							"size": "2.125rem",
							"slug": "x-small"
						},
						{
							"size": "clamp(2.125rem, 10vw, 2.375rem)",
							"slug": "small"
						},
						{
							"size": "clamp(2.125rem, 10vw, 3rem)",
							"slug": "large"
						},
						{
							"size": "clamp(2.125rem, 10vw, 3.25rem)",
							"slug": "x-large"
						}
					]
				}
			},
			"core/query-title": {
				"typography": {
					"fontSizes": [
						{
							"size": "2.125rem",
							"slug": "x-small"
						},
						{
							"size": "clamp(2.125rem, 10vw, 2.375rem)",
							"slug": "small"
						},
						{
							"size": "clamp(2.125rem, 10vw, 3rem)",
							"slug": "large"
						},
						{
							"size": "clamp(2.125rem, 10vw, 3.25rem)",
							"slug": "x-large"
						}
					]
				}
			}
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"width": "1px"
				},
				"color": {
					"text": "var(--wp--preset--color--white)",
					"background": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--typography--font-weight--additional)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--button)",
					"textTransform": "uppercase"
				}
			},
			"core/navigation": {
				"typography": {
					"fontWeight": "500",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--menu)"
				}
			},
			"core/separator": {
				"color": {
					"text": "var(--wp--preset--color--lite-grey)"
				}
			},
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--small)"
					}
				}
			},
			"core/pullquote": {
				"border": {
					"width": "1px 0"
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--primary)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary-font)",
					"fontWeight": "var(--wp--custom--typography--font-weight--heading)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-1)",
					"lineHeight": "var(--wp--custom--typography--line-height--small)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading-big)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary-font)",
					"fontWeight": "var(--wp--custom--typography--font-weight--heading)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-2)",
					"lineHeight": "var(--wp--custom--typography--line-height--medium)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading-normal)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary-font)",
					"fontWeight": "var(--wp--custom--typography--font-weight--heading)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-3)",
					"lineHeight": "var(--wp--custom--typography--line-height--medium)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading-normal)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary-font)",
					"fontWeight": "var(--wp--custom--typography--font-weight--heading)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-4)",
					"lineHeight": "var(--wp--custom--typography--line-height--medium)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading-normal)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary-font)",
					"fontWeight": "var(--wp--custom--typography--font-weight--heading)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-5)",
					"lineHeight": "var(--wp--custom--typography--line-height--medium)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading-normal)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary-font)",
					"fontWeight": "var(--wp--custom--typography--font-weight--heading-6)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-6)",
					"lineHeight": "var(--wp--custom--typography--line-height--medium)",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading-normal)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary-font)",
			"fontSize": "var(--wp--custom--typography--font-size--body)",
			"fontWeight": "var(--wp--custom--typography--font-weight--body)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)",
			"letterSpacing": "var(--wp--custom--typography--letter-spacing--body)"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "products-filter",
			"title": "Products Filter",
			"area": "uncategorized"
		}
	]
}
